        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OP.PRACTICAL_WORK_7"
        mc:Ignorable="d"
        Title="Покушать и точка- Авторизация" Height="450" Width="800">
