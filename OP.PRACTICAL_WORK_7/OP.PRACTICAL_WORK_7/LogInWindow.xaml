        <Button Content="Войти в систему" HorizontalAlignment="Center" Margin="0,236,0,0" VerticalAlignment="Top" Height="41" Width="184" Click="Button_Click"/>
        <Label Content="Покушать и точка- вход в систему" FontSize="21" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" Height="42" Width="368"/>
        <Label Content="Логин" HorizontalAlignment="Center" Margin="0,108,0,0" VerticalAlignment="Top"/>
        <Label Content="Пароль" HorizontalAlignment="Center" Margin="0,164,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
